    --royalty egg
    function autoHatch()
        while _G.autoHatch == true do
        game:GetService("ReplicatedStorage").Events.OpenEgg:FireServer(_G.selectEgg,true,true)
        wait(0.5)
        end
    end
    
    
    --mystery egg v3
    function autoHatch()
        while _G.autoHatch == true do
        game:GetService("ReplicatedStorage").Events.OpenEgg:FireServer(_G.selectEgg2,true,true)
        wait(0.5)
        end
    end
    
    Eggs:AddToggle({
    Name = "Auto Hatch",
        Default = false,
            Callback = function(Value)
                _G.autoHatch = Value
                autoHatch()
        end    
